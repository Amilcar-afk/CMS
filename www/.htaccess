RewriteEngine on

RewriteCond %{REQUEST_FILENAME} !-d
RewriteCond %{REQUEST_FILENAME} !-f

RewriteRule . index.php [L]


<Files ~ "\.sql$">
    Order allow,deny
    Deny from all
</Files>

<Files ~ "\.json$">
    Order allow,deny
    Deny from all
</Files>

<Files ~ "\.lock$">
    Order allow,deny
    Deny from all
</Files>


<Files ~ "\.yml$">
    Order allow,deny
    Deny from all
</Files>